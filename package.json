{
   "name": "taktix",
   "version": "1.0.0",
   "description": "A small browser game written in TypeScript.",
   "repository": "https://github.com/matthieu-locussol/starter-next-typescript",
   "author": "Matthieu LOCUSSOL",
   "license": "MIT",
   "private": false,
   "scripts": {
      "dev": "yarn run next dev",
      "build": "yarn run next build",
      "start": "yarn run next start",
      "export": "yarn run next export",
      "tests": "jest",
      "tests:watch": "jest --watch",
      "check:prettier": "prettier --list-different --config ./.prettierrc.json --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx}\"",
      "check:eslint": "eslint --ignore-path .gitignore .",
      "check:typescript": "tsc --project ./tsconfig.json --noEmit",
      "storybook": "start-storybook -p 6006",
      "build-storybook": "build-storybook"
   },
   "pre-commit": [
      "check:typescript",
      "check:eslint",
      "check:prettier"
   ],
   "dependencies": {
      "@next-auth/prisma-adapter": "^0.5.2",
      "@prisma/client": "2.26.0",
      "@storybook/react": "^6.3.2",
      "axios": "^0.21.1",
      "next": "^11.0.1",
      "next-auth": "^3.27.1",
      "react": "^17.0.1",
      "react-dom": "^17.0.1",
      "swr": "^0.5.6",
      "tailwindcss": "^2.2.4"
   },
   "devDependencies": {
      "@babel/core": "^7.14.6",
      "@storybook/addon-actions": "^6.3.2",
      "@storybook/addon-essentials": "^6.3.2",
      "@storybook/addon-links": "^6.3.2",
      "@storybook/addon-postcss": "^2.0.0",
      "@types/jest": "^26.0.24",
      "@types/node": "^15.14.0",
      "@types/react": "^17.0.13",
      "@types/react-dom": "^17.0.1",
      "@typescript-eslint/eslint-plugin": "^4.28.1",
      "@typescript-eslint/parser": "^4.28.1",
      "autoprefixer": "^10.2.6",
      "babel-loader": "^8.2.2",
      "eslint": "^7.30.0",
      "eslint-config-airbnb-typescript": "^12.3.1",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-import": "^2.23.4",
      "eslint-plugin-jsx-a11y": "^6.4.1",
      "eslint-plugin-react": "^7.22.0",
      "eslint-plugin-react-hooks": "^4.2.0",
      "jest": "^27.0.6",
      "postcss": "^8.3.5",
      "pre-commit": "^1.2.2",
      "prettier": "^2.2.1",
      "prisma": "2.26.0",
      "ts-jest": "^27.0.3",
      "typescript": "4.3.5"
   }
}
