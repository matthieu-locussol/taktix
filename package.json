{
   "name": "taktix",
   "version": "1.0.0",
   "description": "A small browser game written in TypeScript.",
   "repository": "https://github.com/matthieu-locussol/starter-next-typescript",
   "author": "Matthieu LOCUSSOL",
   "license": "MIT",
   "private": false,
   "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "export": "next export",
      "tests": "jest",
      "tests:watch": "jest --watch",
      "tests:e2e": "cypress open",
      "tests:e2e:ci": "cypress run",
      "check:prettier": "prettier --list-different --config ./.prettierrc.json --ignore-path .gitignore --write \"**/*.{js,jsx,ts,tsx}\"",
      "check:eslint": "eslint --ignore-path .gitignore .",
      "check:typescript": "tsc --project ./tsconfig.json --noEmit",
      "storybook": "start-storybook -p 6006",
      "build-storybook": "build-storybook"
   },
   "pre-commit": [
      "check:typescript",
      "check:eslint",
      "check:prettier"
   ],
   "dependencies": {
      "@next-auth/prisma-adapter": "^0.5.3",
      "@prisma/client": "3.0.2",
      "@storybook/react": "^6.3.4",
      "axios": "^0.21.1",
      "next": "^12.1.0",
      "next-auth": "^3.27.3",
      "react": "^17.0.1",
      "react-dom": "^17.0.1",
      "swr": "^1.0.0",
      "tailwindcss": "^2.2.4"
   },
   "devDependencies": {
      "@babel/core": "^7.14.6",
      "@storybook/addon-actions": "^6.3.4",
      "@storybook/addon-essentials": "^6.3.4",
      "@storybook/addon-links": "^6.3.4",
      "@storybook/addon-postcss": "^2.0.0",
      "@types/jest": "^27.0.1",
      "@types/node": "^16.3.3",
      "@types/react": "^17.0.14",
      "@types/react-dom": "^17.0.9",
      "@types/tailwindcss": "^2.2.1",
      "@typescript-eslint/eslint-plugin": "^4.28.3",
      "@typescript-eslint/parser": "^4.31.0",
      "autoprefixer": "^10.3.1",
      "babel-loader": "^8.2.2",
      "cypress": "^8.3.1",
      "eslint": "^7.31.0",
      "eslint-config-airbnb": "^18.2.1",
      "eslint-config-airbnb-base": "^14.2.1",
      "eslint-config-airbnb-typescript": "^14.0.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-cypress": "^2.11.3",
      "eslint-plugin-import": "^2.23.4",
      "eslint-plugin-jsx-a11y": "^6.4.1",
      "eslint-plugin-react": "^7.22.0",
      "eslint-plugin-react-hooks": "^4.2.0",
      "jest": "^27.0.6",
      "postcss": "^8.3.5",
      "pre-commit": "^1.2.2",
      "prettier": "^2.2.1",
      "prisma": "3.0.2",
      "ts-jest": "^27.0.3",
      "typescript": "4.4.2"
   }
}
